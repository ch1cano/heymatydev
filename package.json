{
  "name": "Heydaddy-Backend",
  "version": "1.0.1",
  "description": "Heydaddy Backend.",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production pm2 start server.js",
    "start-cron": "cross-env NODE_ENV=production pm2 start bree.js",
    "mocha": "nyc mocha --timeout=5000 --exit",
    "test": "npm run coverage:clean && npm run test:unit && npm run test:e2e && npm run coverage",
    "test:unit": "cross-env NODE_ENV=test jest --coverage",
    "test:e2e": "cross-env NODE_ENV=test npm run fresh && npm run mocha",
    "dev": "cross-env NODE_ENV=development nodemon --inspect=9230 server.js",
    "fresh": "npm run clean && npm run seed",
    "clean": "node clean.js",
    "seed": "node seed.js",
    "prettier": "prettier --write --config .prettierrc.json \"**/*.js\"",
    "lint": "eslint --fix --config .eslintrc.json \"**/*.js\"",
    "remark": "remark . -o",
    "coverage": "npm run coverage:merge && npm run coverage:merge-report",
    "coverage:clean": "rm -rf .nyc_output && rm -rf coverage",
    "coverage:merge": "istanbul-merge --out coverage/merged/coverage-final.json ./coverage/unit/coverage-final.json ./coverage/e2e/coverage-final.json",
    "coverage:merge-report": "nyc report --reporter=lcov --reporter=text --reporter=json --temp-dir=./coverage/merged --report-dir=./coverage/merged",
    "client": "npm run start --prefix client2",
    "dev:client": "concurrently \"npm run dev\" \"npm run client\""
  },
  "nyc": {
    "reporter": [
      "json",
      "text",
      "lcov"
    ],
    "report-dir": "coverage/e2e",
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "**/*.test.js",
      "jest.config.js",
      "**/data/**",
      "**/node_modules/**",
      "**/.history/**",
      "**/test/**",
      "**/coverage/**",
      "**/tmp/**"
    ],
    "all": true
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@ladjs/graceful": "^1.0.5",
    "babel-eslint": "^10.1.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "bree": "^6.2.1",
    "chalk": "4.1.2",
    "coinbase-commerce-node": "^1.0.4",
    "commander": "^9.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-fetch": "^3.1.4",
    "date-fns": "^2.23.0",
    "dotenv-safe": "^8.2.0",
    "ejs": "^3.1.6",
    "expeditious-engine-redis": "^0.1.2",
    "express": "^4.17.1",
    "express-expeditious": "^5.1.1",
    "express-validator": "^6.12.1",
    "helmet": "^4.6.0",
    "i18n": "^0.13.3",
    "inquirer": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "moment": "^2.29.1",
    "mongoose": "^5.13.5",
    "mongoose-aggregate-paginate-v2": "^1.0.42",
    "mongoose-paginate-v2": "^1.4.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.6.3",
    "nodemailer-mailgun-transport": "^2.1.3",
    "passport": "^0.4.1",
    "passport-google-idtoken": "^1.0.2",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "postcss-cli": "^8.3.1",
    "randomstring": "^1.2.1",
    "request-ip": "^2.1.3",
    "sendmail": "^1.6.1",
    "sharp": "^0.28.3",
    "socket.io": "^4.1.3",
    "socketio-file-upload": "^0.7.3",
    "stripe": "^7.63.1",
    "swagger-ui-express": "^5.0.0",
    "tailwindcss": "^2.2.7",
    "trim-request": "^1.0.6",
    "uuid": "^8.3.2",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "concurrently": "^6.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-formidable": "^4.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "husky": "^4.3.8",
    "istanbul-merge": "^1.1.1",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mocha": "^8.4.0",
    "mongo-seeding": "^3.7.0",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "prettier-eslint": "^12.0.0",
    "remark-cli": "^9.0.0"
  },
  "keywords": [
    "javascript",
    "api",
    "node",
    "express",
    "mongo",
    "mongodb",
    "jwt",
    "postman",
    "i18n",
    "jwt-authentication",
    "token",
    "eslint",
    "starter",
    "web",
    "app",
    "mongoose",
    "rest",
    "skeleton",
    "async",
    "await",
    "mvp",
    "front-end",
    "testing",
    "prettier",
    "mocha",
    "chai",
    "redis",
    "JSDoc"
  ]
}
